@model ConfigurationModel

<div class="card-body">
    <div class="form-group row">
        <div class="col-md-3">
            <nop-override-store-checkbox asp-for="MJSTradersLogoId_OverrideForStore" asp-input="MJSTradersLogoId" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <nop-label asp-for="MJSTradersLogoId" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="MJSTradersLogoId" />
            <span asp-validation-for="MJSTradersLogoId"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-override-store-checkbox asp-for="MJSTradersNTNNumber_OverrideForStore" asp-input="MJSTradersNTNNumber" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <nop-label asp-for="MJSTradersNTNNumber" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="MJSTradersNTNNumber" />
            <span asp-validation-for="MJSTradersNTNNumber"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <nop-override-store-checkbox asp-for="MJSTradersSTRNNo_OverrideForStore" asp-input="MJSTradersSTRNNo" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <nop-label asp-for="MJSTradersSTRNNo" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="MJSTradersSTRNNo" />
            <span asp-validation-for="MJSTradersSTRNNo"></span>
        </div>
    </div>
</div>