@model SalesQuotationModel

@{
    Layout = "_AdminLayout";
    //page title
    ViewBag.Title = string.Format("{0} / {1}", T("Nop.Cypher.Misc.MJSTraders.Title").Text, T("Nop.Cypher.Misc.MJSTraders.Admin.SalesQuotation.Edit.Page.Title").Text);
    //active menu item (system name)
    NopHtml.SetActiveMenuItemSystemName("Cypher.MJSTraders.SalesQuotation");
}

<form asp-controller="SalesQuotation" asp-action="Edit" method="post">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("Nop.Cypher.Misc.MJSTraders.Title") - @T("Nop.Cypher.Misc.MJSTraders.Admin.SalesQuotation.Edit.Title")
            <small>
                <i class="fas fa-arrow-circle-left"></i>
                <a asp-action="CustomerEdit" asp-route-id="@Model.SalesQuotationCustomerId">@T("Nop.Cypher.Misc.MJSTraders.Admin.SalesQuatationCustomer.BackTo")</a>
            </small>
        </h1>
        <div class="float-right">
            <button type="submit" name="save" class="btn btn-primary">
                <i class="far fa-save"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn btn-primary">
                <i class="far fa-save"></i>
                @T("Admin.Common.SaveContinue")
            </button>
            @*<span id="sales-quotation-delete" class="btn btn-danger">
                    <i class="far fa-trash-alt"></i>
                    @T("Admin.Common.Delete")
                </span>*@
            <a asp-action="SendNotification" asp-route-id="@Model.Id" class="btn btn-success">
                <i class="far fa-plus-square"></i>
                @T("SendNotification")
            </a>
        </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate.cshtml", Model)
</form>
<nop-delete-confirmation asp-model-id="@Model.Id" asp-button-id="sales-quotation-delete" />
