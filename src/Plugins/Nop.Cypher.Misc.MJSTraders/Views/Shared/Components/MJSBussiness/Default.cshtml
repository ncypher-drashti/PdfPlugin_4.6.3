@model MJSBussinessModel
@{
    NopHtml.AddScriptParts(ResourceLocation.Footer, "~/Plugins/Cypher.MJSTraders/Content/js/slick.min.js");
    NopHtml.AddCssFileParts("~/Plugins/Cypher.MJSTraders/Content/css/slick.css");
    NopHtml.AddCssFileParts("~/Plugins/Cypher.MJSTraders/Content/css/slick-theme.css");
}

@if (!string.IsNullOrEmpty(Model.ChooseYourBusinessText) || !string.IsNullOrEmpty(Model.LearnAboutText))
{
    <div class="twoSecBlock">
        <div class="twoSecInner">
            <div class="twoSecMain">
                @if (!string.IsNullOrEmpty(Model.ChooseYourBusinessText))
                {
                    <div class="ChooseYourBusinessText">
                        @Html.Raw(Model.ChooseYourBusinessText)
                    </div>
                }
                @if (!string.IsNullOrEmpty(Model.LearnAboutText))
                {
                    <div class="LearnAboutText">
                        @Html.Raw(Model.LearnAboutText)
                    </div>
                }
            </div>
        </div>
    </div>
}




