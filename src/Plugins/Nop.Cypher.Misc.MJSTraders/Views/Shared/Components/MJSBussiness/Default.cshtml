<script asp-location="Footer">
    $(".chooser-yr-bussiness-slider").slick({
        dots: false,
        infinite: true,
        speed: 400,
        arrows: true,
        adaptiveHeight: true,
        slidesToShow: 2,
        slidesToScroll: 1,
        vertical: true,
        responsive: [
            {
                breakpoint: 1701,
                settings: {
                    slidesToShow: 1,
                    slidesToScroll: 1
                }
            }
        ]
    });
</script>